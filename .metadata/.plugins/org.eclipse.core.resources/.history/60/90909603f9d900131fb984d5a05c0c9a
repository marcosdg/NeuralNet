package core;

public class Connection {

	private Neuron source;
	private Neuron target;
	private Weight weight;
	
	
	public Connection(Neuron source, Neuron target) {
		
		if (source != null && target != null) {
			this.source = source;
			this.target = target;
			this.weight = new Weight();
		}
	}
	
}
